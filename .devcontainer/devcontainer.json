{
    "name": "ansible-dev-container-docker",
    "image": "ghcr.io/ansible/community-ansible-dev-tools:latest",
    "containerUser": "root",
    "runArgs": [
      "--privileged",
      "--device",
      "/dev/fuse",
      "--hostname=ansible-dev-container"
    ],
    "updateRemoteUserUID": true,
    "customizations": {
      "vscode": {
        "extensions": ["redhat.ansible"]
      }
    }
  }